<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.catering.mapper.GasTypeMapper">
    
    <resultMap type="GasType" id="GasTypeResult">
        <result property="typeId"    column="type_id"    />
        <result property="name"    column="name"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectGasTypeVo">
        select type_id, name, status, create_by, create_time, update_by, update_time, remark from cy_gas_type
    </sql>

    <select id="selectGasTypeList" parameterType="GasType" resultMap="GasTypeResult">
        <include refid="selectGasTypeVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name = #{name}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectGasTypeById" parameterType="Long" resultMap="GasTypeResult">
        <include refid="selectGasTypeVo"/>
        where type_id = #{typeId}
    </select>
        
    <insert id="insertGasType" parameterType="GasType" useGeneratedKeys="true" keyProperty="typeId">
        insert into cy_gas_type
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">name,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">#{name},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateGasType" parameterType="GasType">
        update cy_gas_type
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where type_id = #{typeId}
    </update>

    <delete id="deleteGasTypeById" parameterType="Long">
        delete from cy_gas_type where type_id = #{typeId}
    </delete>

    <delete id="deleteGasTypeByIds" parameterType="String">
        delete from cy_gas_type where type_id in 
        <foreach item="typeId" collection="array" open="(" separator="," close=")">
            #{typeId}
        </foreach>
    </delete>

</mapper>