<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.catering.mapper.WarnMsgMapper">
    
    <resultMap type="WarnMsg" id="WarnMsgResult">
        <result property="id"    column="id"    />
        <result property="type"    column="type"    />
        <result property="feedbackId"    column="feedback_id"    />
        <result property="restaurantId"    column="restaurant_id"    />
        <result property="content"    column="content"    />
        <result property="msgType"    column="msg_type"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectWarnMsgVo">
        select id, type, feedback_id, restaurant_id, content, msg_type, status, create_by, create_time, update_by, update_time, remark from cy_warn_msg
    </sql>

    <select id="selectWarnMsgList" parameterType="WarnMsg" resultMap="WarnMsgResult">
        <include refid="selectWarnMsgVo"/>
        <where>  
            <if test="type != null "> and type = #{type}</if>
            <if test="feedbackId != null "> and feedback_id = #{feedbackId}</if>
            <if test="restaurantId != null "> and restaurant_id = #{restaurantId}</if>
            <if test="msgType != null "> and msg_type = #{msgType}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectWarnMsgById" parameterType="Long" resultMap="WarnMsgResult">
        <include refid="selectWarnMsgVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertWarnMsg" parameterType="WarnMsg" useGeneratedKeys="true" keyProperty="id">
        insert into cy_warn_msg
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="type != null">type,</if>
            <if test="feedbackId != null">feedback_id,</if>
            <if test="restaurantId != null">restaurant_id,</if>
            <if test="content != null">content,</if>
            <if test="msgType != null">msg_type,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="type != null">#{type},</if>
            <if test="feedbackId != null">#{feedbackId},</if>
            <if test="restaurantId != null">#{restaurantId},</if>
            <if test="content != null">#{content},</if>
            <if test="msgType != null">#{msgType},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateWarnMsg" parameterType="WarnMsg">
        update cy_warn_msg
        <trim prefix="SET" suffixOverrides=",">
            <if test="type != null">type = #{type},</if>
            <if test="feedbackId != null">feedback_id = #{feedbackId},</if>
            <if test="restaurantId != null">restaurant_id = #{restaurantId},</if>
            <if test="content != null">content = #{content},</if>
            <if test="msgType != null">msg_type = #{msgType},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteWarnMsgById" parameterType="Long">
        delete from cy_warn_msg where id = #{id}
    </delete>

    <delete id="deleteWarnMsgByIds" parameterType="String">
        delete from cy_warn_msg where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>